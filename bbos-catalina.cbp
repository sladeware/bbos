				<Option compiler="catalina" />
				<Compiler>
					<Add directory="include" />
					<Add directory="samples\demo_itc" />
				</Compiler>
			</Target>
			<Target title="Debug">
				<Option output="bin\Debug\bbos-catalina" prefix_auto="1" extension_auto="0" />
				<Option object_output="obj\Debug\" />
				<Option type="1" />
				<Option compiler="catalina" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add option="-g" />
				</Linker>
			</Target>
			<Environment>
